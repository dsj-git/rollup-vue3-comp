{
  "name": "rollup-vue3-comp",
  "version": "1.1.3",
  "private": false,
  "author": "dsj",
  "main": "dist/rollup-vue3-comp.umd.js",
  "module": "dist/rollup-vue3-comp.esm.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "dev": "vue-cli-service serve",
    "lint": "vue-cli-service lint",
    "build": "npm run clean && npm run build:esm && npm run build:umd",
    "build:esm": "rollup --config ./rollup.esm.config.js",
    "build:umd": "rollup --config ./rollup.umd.config.js",
    "clean": "rimraf ./dist"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/node": "^20.8.9",
    "@vitejs/plugin-vue": "^4.4.0",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "core-js": "^3.8.3",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3",
    "rimraf": "^5.0.5",
    "rollup-plugin-css-only": "^4.3.0",
    "rollup-plugin-vue": "^6.0.0",
    "typescript": "^5.2.2",
    "unplugin-auto-import": "^0.16.7",
    "vite": "^4.5.0",
    "vue": "^3.2.13"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dsj-git/rollup-vue3-comp.git"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}
